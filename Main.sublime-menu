[
  {
    "caption": "Preferences",
    "id": "preferences",
    "mnemonic": "n",
    "children":
    [
      {
        "caption": "Package Settings",
        "id": "package-settings",
        "mnemonic": "P",
        "children":
        [
          {
            "caption": "TwoColumns",
            "children":
            [
              {
                "caption": "Key Bindings",
                "command": "edit_settings",
                "args": {
                  "base_file": "${packages}/TwoColumns/Default ($platform).sublime-keymap",
                  "default": "// Settings in here override those in \"TwoColumns/Default.sublime-settings\",\n\n{\n\t$0\n}\n"
                }                                
              }
            ]
          }
        ]
      }
    ]
  }
